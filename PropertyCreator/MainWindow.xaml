<Window x:Class="PropertyCreator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Property Creator" Height="401" Width="452">
    <Grid>
        <TextBox Height="110" Margin="14,57,13,0" Name="textBox1" VerticalAlignment="Top" AcceptsReturn="True" AcceptsTab="True" />
        <Button Content="&gt;&gt;&gt;" Height="23" HorizontalAlignment="Left" Margin="14,173,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
        <TextBox Margin="14,202,13,12" Name="textBox2" />
        <CheckBox Content="Add Property Changed" Height="16" HorizontalAlignment="Left" Margin="14,12,0,0" Name="propertyChangedCheck" VerticalAlignment="Top" IsEnabled="True" IsChecked="True" />
        <CheckBox x:Name="SimpleNotifyCheckBox" Content="SimpleNotifyClass" HorizontalAlignment="Left" Margin="18,37,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <ComboBox x:Name="MemberNameComboBox" HorizontalAlignment="Left" Margin="210,10,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="Camel Case" IsSelected="True"/>
            <ComboBoxItem Content="Start Underscore"/>
            <ComboBoxItem Content="End Underscore"/>
        </ComboBox>
    </Grid>
</Window>
